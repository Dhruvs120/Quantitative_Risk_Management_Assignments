    "    dS2t= iDF/(iDF-2)\n",
    "    dC= dSy / np.sqrt(dS2t)\n",
    "    vT= dMu+dC*st.t.rvs(df= iDF, size= 10000000)\n",
    "    print (f'Simulating t({iDF}), mean= {vT.mean()}, std= {vT.std()}')\n",
    "\n",
    "    dVaRt= np.quantile(vT, .95)\n",
    "    vI= vT >= dVaRt\n",
    "    print (f'Sim, vart= {dVaRt}, ES= {vT[vI].mean()}')\n",
    "\n",
    "    np.quantile(vT, .99)\n",
    "    VaR(.95, 0, 1)\n"
   ]
